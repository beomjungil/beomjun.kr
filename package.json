{
  "name": "beomjun-kr",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "paraglide-js compile --project ./project.inlang && pnpm run sync && astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "i18n:sync": "paraglide-js compile --project ./project.inlang",
    "sync": "astro sync && pnpm run i18n:sync && pnpm lint:fix",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:migrate:local": "wrangler d1 migrations apply beomjun-kr --local",
    "db:migrate:prod": "wrangler d1 migrations apply beomjun-kr",
    "db:migrate:preview": "wrangler d1 migrations apply --env preview preview-beomjun-kr",
    "db:studio:local": "cross-env LOCAL_DB_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) drizzle-kit studio",
    "db:studio:preview": "cross-env DB_NAME='preview-beomjun-kr' WRANGLER_CONFIG='--environment=preview' drizzle-kit studio",
    "db:studio:prod": "drizzle-kit studio",
    "postinstall": "paraglide-js compile --project ./project.inlang && pnpm run sync"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.6",
    "@astrojs/cloudflare": "^9.1.0",
    "@astrojs/mdx": "^2.1.1",
    "@astrojs/sitemap": "^3.1.1",
    "@astrojs/tailwind": "^5.1.0",
    "@fec/remark-a11y-emoji": "^4.0.2",
    "@inlang/paraglide-js-adapter-astro": "^0.1.6",
    "@lucia-auth/adapter-drizzle": "^1.0.4",
    "@shikijs/transformers": "^1.1.7",
    "@tailwindcss/typography": "^0.5.10",
    "@vercel/og": "^0.6.2",
    "arctic": "^1.2.1",
    "astro": "^4.4.15",
    "autoprefixer": "^10.4.18",
    "awilix": "^10.0.1",
    "cssnano": "^6.1.0",
    "drizzle-orm": "^0.30.2",
    "github-slugger": "^2.0.0",
    "lucia": "^3.1.1",
    "lucide-astro": "^0.350.0",
    "neverthrow": "^6.1.0",
    "oslo": "^1.1.3",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-preset-minify": "^7.0.0",
    "rehype-pretty-code": "^0.13.0",
    "rehype-slug": "^6.0.0",
    "remark-behead": "^3.1.0",
    "remark-join-cjk-lines": "^1.0.11",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-pattern": "^5.0.8",
    "tw-colors": "^3.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240314.0",
    "@eslint/js": "^8.57.0",
    "@inlang/paraglide-js": "1.2.7",
    "@types/node": "^20.11.25",
    "astro-eslint-parser": "^0.16.3",
    "better-sqlite3": "^9.4.3",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "type-fest": "^4.11.1",
    "typescript": "^5.4.2",
    "typescript-eslint": "^7.1.1",
    "wrangler": "^3.34.2"
  }
}
