{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "moduleResolution": "Bundler",
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.astro",
    "**/*.d.ts",
    "src/env.d.ts",
    "src/pages/.well-known/**/*"
  ]
}