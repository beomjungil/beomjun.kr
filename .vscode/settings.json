{
  "i18n-ally.localesPaths": [
    "src/i18n"
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "astro",
    "typescript",
    "typescriptreact"
  ],
  "eslint.experimental.useFlatConfig": true,
  "prettier.documentSelectors": [
    "**/*.astro"
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.classpath": true,
    "**/node_modules": false,
    "**/.factorypath": true
  }
}